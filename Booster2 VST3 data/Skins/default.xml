    <IF condition="$SCRIPT_EDIT_MODE$">
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/skin_scanner/skin_scanner.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/controls_gen/controls_gen.inc" />
