    </IF>

    <!-- if MAKE_RESIZEABLE then $RE100$ = 100%, otherwise empty-->
    <CELL width="$RE100$" height="$RE100$">
      
      <!-- include toolbar -->
      <INC file="$LM_DIR$/head-toolbar.inc" />
      
      <!-- include main body -->
      <INC file="$LM_DIR$/main-body.inc" />

      <!-- include status bar -->
      <INC file="$LM_DIR$/tail-statusbar.inc" />
    </CELL>

